Maximum Subarray
------------------------
Time Complexity - O(n)
Space Complexity - O(1)
------------------------
class Solution {
    public int maxSubArray(int[] nums) {
        int curr_max = nums[0];
        int global_max = nums[0];

        for(int i=1;i<nums.length;i++){
            curr_max= Math.max(curr_max + nums[i], nums[i]);
            global_max = Math.max(global_max,curr_max);
        }
        return global_max;
    }
}
