Number of One Bits
------------------------
Time Complexity - O(1)
Space Complexity - O(1)
------------------------
class Solution {
    public int hammingWeight(int n) {
        int count = 0;

        for(int i=0;i<32;i++){
            if(((1 << i) & n) != 0){
                count++;
            }
        } 
        return count;
    }
}
