Reverse Bits
-----------------------
Time Complexity -O(1)
Space Complexity -O(1)
-----------------------
class Solution {
    public int reverseBits(int n) {
        int ans =0;
        for(int i=0;i<32;i++){
// If current bit is set, set 31-ith bit in ans variable storing reversed bit 
            if(((n>>i) & 1) == 1){
// Use OR operator to avoid overwriting bit
                ans |= 1 << (31-i);
            }
        }
        return ans;

    }
}
