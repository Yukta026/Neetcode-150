Sum of Two Integers
----------------------
Time Complexity - O(1)
Space Complexity - O(1)
----------------------
class Solution {
    public int getSum(int a, int b) {
        int res = 0;
        int carry = 0;

      
        for(int i=0;i<32;i++){
            int ai = (a >> i) & 1;
            int bi = (b >> i) & 1;
            int sumBit = ai ^ bi ^ carry;
            res |= sumBit << i;

            carry = (ai & bi) | (carry & (ai ^ bi));
        }
        return res;
    }
}
